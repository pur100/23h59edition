// GENERAL //

  #main {
    background-color: #e1d5ce;
  }

// HEADER //

  .gif_logo {
    position: absolute;
    width: 200px;
    top: 10px;
    left: 10px;
    display: flex;
    justify-content: space-between;
    width: 250px;

    .gif img {
      height: 50px;
    }
  }

  .Header__Wrapper {
    position: relative;
  }




// MOODBOARD //

  #shopify-section-custom_moodboard {
    margin-top: 200px;

    .mood_container {
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: space-around;
    }

    .mood_row_1, .mood_row_2 {
      display:flex;
      justify-content: space-between;
      width: 90%;
    }

    .image_a, .image_c {
      width: 250px;
      height: 500px;
      margin: 5px;
    }

    .image_b {
      height: 500px;
      width: 750px;
      margin: 5px;
    }

    .image_e, .image_d {
      height: 300px;
      width: 625px;
      margin: 5px;
    }
  }

